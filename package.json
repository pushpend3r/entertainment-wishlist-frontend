{
  "name": "entertainment-wishlist-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test:watch": "npm run graphql-codegen && NODE_OPTIONS='--no-deprecation' vitest",
    "test": "npm run graphql-codegen && NODE_OPTIONS='--no-deprecation' vitest run",
    "dev": "vite",
    "build": "npm run graphql-codegen && npm run test && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "graphql-codegen": "graphql-codegen --config graphql-codegen-config.ts",
    "graphql-codegen:watch": "npm run graphql-codegen -- --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.10.2",
    "@uidotdev/usehooks": "^2.4.1",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.6",
    "@parcel/watcher": "^2.4.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^24.1.0",
    "react-router-dom": "^6.23.0",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^1.6.0"
  }
}
